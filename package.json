  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siljeangelvik/ca-workflow-demo.git"
  },
  "keywords": [
    "demo",
    "course assignment",
    "workflow"
  ],
  "author": "Silje A-",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/siljeangelvik/ca-workflow-demo/issues"
  },
  "homepage": "https://github.com/siljeangelvik/ca-workflow-demo#readme",
  "dependencies": {
    "one-liner-joke": "^1.2.2"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
