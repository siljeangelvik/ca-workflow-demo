{
  "name": "ca-workflow-demo",
  "version": "1.0.0",
  "description": "demo for course assignment: workflow",
  "type": "module",
  "entry-point": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
